{
    "languageserver": {
        "svls": {
            "command": "svls",
            "filetypes": ["systemverilog"]
        },
        "clangd": {
            "command": "clangd",
            "trace.server": "verbose",
            "rootPatterns": ["compile_flags.txt", "compile_commands.json", ".git/", ".hg/"],
